{
    "name": "HiFiGAN",
    "n_gpu": 1,
    "arch": {
      "type": "HiFiGAN",
      "args": {
        "mpd_periods": [2, 3, 5, 7, 11],
        "msd_num_scales": 3,
        "upscale_kernels": [16, 16, 4, 4],
        "upscale_init_height": 512,
        "residual_kernels": [3, 7, 11],
        "residual_dilations": [
          [[1, 1], [3, 1], [5, 1]], [[1, 1], [3, 1], [5, 1]], [[1, 1], [3, 1], [5, 1]]
        ]
      }
    },
    "loss": {
      "type": "HiFiGANLoss",
      "args": {
        "fm_loss_weight": 2,
        "mel_loss_weight": 45
      }
    },
    "G_scheduler": {
      "type": "ExponentialLR",
      "args": {
        "gamma": 0.9
      }
    },
    "D_scheduler": {
      "type": "ExponentialLR",
      "args": {
        "gamma": 0.9
      }
    },
    "G_optimizer": {
      "type": "AdamW",
      "args": {
        "lr": 9e-5,
        "weight_decay": 1e-2,
        "betas": [0.8, 0.99]
      }
    },
    "D_optimizer": {
      "type": "AdamW",
      "args": {
        "lr": 9e-5,
        "weight_decay": 1e-2,
        "betas": [0.8, 0.99]
      }
    },
    "trainer": {
      "epochs": 250,
      "len_epoch": 100,
      "grad_norm_clip": 10,
      "monitor": "min val_loss",
      "early_stop": 500,
      "save_dir": "saved/",
      "save_period": 50,
      "log_parameters": false,
      "verbosity": 2,
      "visualize": "wandb",
      "wandb_entity": "yaraksen",
      "wandb_project": "hifigan"
    },
    "data": {
      "train": {
        "batch_size": 16,
        "num_workers": 4,
        "datasets": [{
            "type": "HiFiLJSpeech",
            "args": {
              "data_path": "/kaggle/input/fastspeecch-dataset/data/data/LJSpeech-1.1"
            }}
        ]
      }
    },
    "augmentations": {
      "wave": [],
      "spectrogram": []
    },
    "metrics": []
  }
  